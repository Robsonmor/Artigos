@model IEnumerable<DeWebFormsParaMvc.Dominio.EstiloMusical>

@{
    ViewBag.Title = "Index";
}

<h2>Estilos Musicais</h2>

<table>
    <tr>
        <th>
            Id
        </th>
        <th>
            Nome
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
    </tr>
}

</table>
